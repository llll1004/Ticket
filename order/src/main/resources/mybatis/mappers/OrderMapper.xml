        <result column="hall" property="hall" />
        <result column="seat" property="seat" />
        <result column="status" property="status" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, orderno, ordertime, userid, money, paytime, cinema, hall, seat, status
    </sql>

</mapper>
