<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.woniuxy.order.mapper.OrderMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.woniuxy.order.entity.Order">
        <id column="id" property="id" />
        <result column="order_no" property="orderNo" />
        <result column="order_time" property="orderTime" />
        <result column="user_id" property="userId" />
        <result column="money" property="money" />
        <result column="pay_time" property="payTime" />
        <result column="cinema" property="cinema" />
        <result column="hall" property="hall" />
        <result column="seat" property="seat" />
        <result column="status" property="status" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, order_no, order_time, user_id, money, pay_time, cinema, hall, seat, status
    </sql>

</mapper>
